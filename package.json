{
  "name": "linkedin-rss",
  "version": "1.0.0",
  "description": "LinkedIn RSS GitHub Action",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lint": "eslint . --fix --format pretty"
  },
  "keywords": [
    "github-action",
    "linkedin",
    "rss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mrmcmuffinz/Linkedin-RSS.git"
  },
  "bugs": {
    "url": "https://github.com/mrmcmuffinz/Linkedin-RSS/issues"
  },
  "author": "mrmcmuffinz",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-pretty": "^6.0.1",
    "globals": "^16.5.0",
    "nock": "^13.5.6",
    "prettier": "^3.4.2",
    "rollup": "^4.28.1",
    "vitest": "^4.0.14"
  }
}
